{
  "name": "fantasy-baseball-draft",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npx webpack --mode production",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "webpack serve --mode development",
    "dev:backend": "node server-local.js",
    "start:prod": "concurrently \"npm run start:backend\" \"npm run build && serve -s dist\"",
    "start:backend": "cd backend && node fixed-server.js",
    "start:frontend": "webpack serve --mode development",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.3.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "pg": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tabs": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.20.0",
    "babel-loader": "^9.1.0",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.20",
    "serve": "^14.2.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.0"
  }
}