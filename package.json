{
    "name": "fantasy-baseball-draft",
    "version": "1.0.0",
    "description": "A utility for fantasy baseball draft data entry and management",
    "main": "src/index.js",
    "scripts": {
      "build": "webpack --mode production",
      "test": "jest",
      "start:frontend": "webpack serve --mode development", 
      "start:backend": "cd backend && node fixed-server.js",
      "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
      "start:prod": "concurrently \"npm run start:backend\" \"npm run build && serve -s dist\""
    },
    "dependencies": {
      "axios": "^1.6.0",
      "firebase": "^10.5.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-tabs": "^6.0.2",
      "concurrently": "^8.2.0",
      "serve": "^14.2.1"
    },
    "devDependencies": {
      "@babel/core": "^7.23.2",
      "@babel/preset-env": "^7.23.2",
      "@babel/preset-react": "^7.22.15",
      "babel-loader": "^9.1.3",
      "css-loader": "^6.8.1",
      "html-webpack-plugin": "^5.5.3",
      "jest": "^29.7.0",
      "style-loader": "^3.3.3",
      "webpack": "^5.89.0",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^4.15.1"
    }
  }